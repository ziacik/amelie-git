{
	"version": 1,
	"projects": {
		"backend": {
			"root": "apps/backend",
			"sourceRoot": "apps/backend/src",
			"projectType": "application",
			"architect": {
				"build": {
					"builder": "@nrwl/node:webpack",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/backend",
						"main": "apps/backend/src/main.ts",
						"tsConfig": "apps/backend/tsconfig.app.json",
						"assets": ["apps/backend/src/assets"]
					},
					"configurations": {
						"production": {
							"optimization": true,
							"extractLicenses": true,
							"inspect": false,
							"fileReplacements": [
								{
									"replace": "apps/backend/src/environments/environment.ts",
									"with": "apps/backend/src/environments/environment.prod.ts"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@nrwl/node:node",
					"options": {
						"buildTarget": "backend:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"outputs": ["{options.outputFile}"],
					"options": {
						"lintFilePatterns": ["apps/backend/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/backend"],
					"options": {
						"jestConfig": "apps/backend/jest.config.ts",
						"passWithNoTests": true
					}
				}
			},
			"tags": []
		},
		"core": {
			"projectType": "library",
			"root": "libs/core",
			"sourceRoot": "libs/core/src",
			"prefix": "core",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/core/src/**/*.ts", "libs/core/src/**/*.html"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "libs/core/jest.config.ts",
						"passWithNoTests": true
					},
					"outputs": ["coverage/libs/core"]
				}
			},
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"tags": []
		},
		"testing": {
			"root": "libs/testing",
			"sourceRoot": "libs/testing/src",
			"projectType": "library",
			"schematics": {},
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/testing/**/*.ts", "libs/testing/src/**/*.html"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "libs/testing/jest.config.ts",
						"passWithNoTests": true
					},
					"outputs": ["coverage/libs/testing"]
				}
			},
			"tags": []
		},
		"web": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "apps/web",
			"sourceRoot": "apps/web/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/apps/web",
						"index": "apps/web/src/index.html",
						"main": "apps/web/src/main.ts",
						"polyfills": "apps/web/src/polyfills.ts",
						"tsConfig": "apps/web/tsconfig.app.json",
						"aot": true,
						"assets": ["apps/web/src/favicon.ico", "apps/web/src/assets"],
						"styles": ["apps/web/src/styles.scss"],
						"scripts": []
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "apps/web/src/environments/environment.ts",
									"with": "apps/web/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							]
						}
					},
					"outputs": ["{options.outputPath}"]
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "web:build",
						"proxyConfig": "apps/web/proxy.conf.json"
					},
					"configurations": {
						"production": {
							"browserTarget": "web:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "web:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/web/src/**/*.ts", "apps/web/src/**/*.html"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "apps/web/jest.config.ts",
						"passWithNoTests": true
					},
					"outputs": ["coverage/apps/web"]
				}
			},
			"tags": []
		},
		"web-e2e": {
			"root": "apps/web-e2e",
			"sourceRoot": "apps/web-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/web-e2e/cypress.json",
						"tsConfig": "apps/web-e2e/tsconfig.e2e.json",
						"devServerTarget": "web:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "web:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/web-e2e/**/*.{js,ts}", "apps/web-e2e/src/**/*.html"]
					}
				}
			},
			"tags": [],
			"implicitDependencies": ["web"]
		}
	}
}
